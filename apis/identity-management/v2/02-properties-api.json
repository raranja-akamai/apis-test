                        "enum": [
                          "cname",
                          "arlfile",
                          "cpcode",
                          "storagegroup",
                          "fpdomain",
                          "edns"
                        ]
                      },
                      "resourceName": {
                        "type": "string",
                        "description": "The name you supply for the resource.\n"
                      },
                      "modifiedDate": {
                        "type": "string",
                        "description": "ISO 8601 timestamp indicating when the resource was last updated.\n",
                        "format": "date-time"
                      }
                    }
                  }
                }
              }
            }
          }
        }
