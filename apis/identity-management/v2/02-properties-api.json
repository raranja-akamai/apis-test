                    "additionalProperties": false,
                    "required": [
                      "uiUserName",
                      "uiIdentityId",
                      "firstName",
                      "lastName",
                      "isBlocked"
                    ],
                    "properties": {
                      "uiUserName": {
                        "type": "string",
                        "description": "The full name for the user, as it appears in interfaces.\n"
                      },
                      "uiIdentityId": {
                        "type": "string",
                        "description": "A unique identifier for a user. Also known as a `contactId` in other APIs.\n"
