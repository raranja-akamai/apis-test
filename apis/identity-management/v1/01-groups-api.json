      },
      "get": {
        "operationId": "get-groups",
        "summary": "List groups",
        "tags": [
          "Groups"
        ],
        "description": "This operation lists all groups the administrator can manage for the current account and contract type or other managed accounts using the `accountSwitchKey` parameter. The account and contract type are determined by the access tokens in your API client.\n",
        "parameters": [
          {
            "name": "actions",
            "in": "query",
            "description": "Includes information about actions such as `edit` or `delete` that you can take for the object, `false` by default. Don't confuse this with the `action` parameter, which applies to the [Set two-factor authentication](ref:put-user-profile-tfa) operation.\n",
            "schema": {
              "type": "boolean",
              "example": true,
              "default": false
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The response provides the groups.\n",
            "content": {
              "application/json": {
                "example": [
                  {
                    "groupId": 12345,
                    "groupName": "Main Street Corporation",
                    "createdDate": "2012-04-28T00:00:00.000Z",
                    "createdBy": "szhang",
                    "modifiedDate": "2012-04-28T00:00:00.000Z",
                    "modifiedBy": "hgildong",
                    "actions": {
                      "edit": true,
                      "delete": false
                    },
                    "subGroups": [
                      {
                        "groupId": 12345,
                        "groupName": "Internet Company",
                        "createdDate": "2013-10-29T19:05:52.000Z",
                        "createdBy": "szhang",
                        "modifiedDate": "2017-07-25T22:30:20.000Z",
                        "modifiedBy": "mrossi",
                        "parentGroupId": 12345,
                        "actions": {
                          "edit": true,
                          "delete": false
                        },
                        "subGroups": [
                          {
                            "groupId": 54321,
                            "groupName": "Digital Company",
                            "createdDate": "2017-07-25T22:30:47.000Z",
                            "createdBy": "jkowalski",
                            "modifiedDate": "2017-07-25T22:30:47.000Z",
                            "modifiedBy": "jkowalski",
                            "parentGroupId": 11111,
                            "subGroups": [],
                            "actions": {
                              "edit": true,
                              "delete": false
                            }
                          }
                        ]
                      }
                    ]
                  }
                ],
                "schema": {
                  "type": "array",
                  "description": "Encapsulates information about a group.\n",
                  "minItems": 0,
                  "items": {
                    "type": "object",
                    "description": "Specifies a group.\n",
                    "additionalProperties": false,
                    "required": [
                      "createdBy",
                      "createdDate",
                      "groupId",
                      "groupName",
                      "modifiedBy",
                      "modifiedDate",
                      "subGroups"
                    ],
                    "properties": {
                      "actions": {
                        "type": "object",
                        "description": "Specifies activities available for the group.\n",
                        "additionalProperties": false,
                        "required": [
                          "delete",
                          "edit"
                        ],
                        "properties": {
                          "delete": {
                            "type": "boolean",
                            "description": "Whether you can remove the group from the account. You can't remove a group that contains resources or subgroups, or if users have roles on that group.\n"
                          },
                          "edit": {
                            "type": "boolean",
                            "description": "Whether you can modify the group.\n"
                          }
                        }
                      },
                      "createdBy": {
                        "type": "string",
                        "description": "The user who created the group.\n",
                        "minLength": 1
                      },
                      "createdDate": {
                        "type": "string",
                        "description": "ISO 8601 timestamp indicating when the group was created.\n",
                        "format": "date-time"
                      },
                      "groupId": {
                        "type": "integer",
                        "description": "Unique identifier for each group.\n",
                        "minimum": 1
                      },
                      "groupName": {
                        "type": "string",
                        "description": "Descriptive label for the group.\n",
                        "minLength": 1
                      },
                      "modifiedBy": {
                        "type": "string",
                        "description": "The user who last edited the group.\n",
                        "minLength": 1
                      },
                      "modifiedDate": {
                        "type": "string",
                        "description": "ISO 8601 timestamp indicating when the group was last updated.\n",
                        "format": "date-time"
                      },
                      "parentGroupId": {
                        "type": "integer",
                        "description": "Unique identifier for the parent group within the group tree. If you're viewing group info for a root-level group, you may not see this member.\n",
                        "minimum": 0
                      },
                      "subGroups": {
                        "type": "array",
                        "description": "Children of the parent group. Permissions cascade downward from parent to child unless the child group's `isBlocked` is `true`.\n",
                        "minItems": 0,
                        "items": {
                          "type": "object",
                          "description": "A further nested group.\n",
                          "additionalProperties": true,
                          "properties": {}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
