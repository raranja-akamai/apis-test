      },
      "parameters": [
        {
          "name": "destinationGroupId",
          "in": "path",
          "description": "Unique identifier for a group you're putting the other group into. To get this value, run the [List groups](ref:get-groups) operation.\n",
          "required": true,
          "schema": {
            "type": "integer",
            "example": 54321
          }
        },
        {
          "name": "sourceGroupId",
          "in": "path",
          "description": "Unique identifier for a group you want to move. To get this value, run the [List groups](ref:get-groups) operation.\n",
          "required": true,
          "schema": {
            "type": "integer",
            "example": 32145
          }
        }
      ],
      "get": {
        "operationId": "get-move-affected-users",
        "summary": "List users affected by moving a group",
        "tags": [
          "Move groups"
        ],
        "description": "This operation lists users who are affected when you move a group. Users may lose or gain access to resources depending on the roles and permissions inherited from the new parent group. Users with a `userType` of `lostAccess` lose their access to the source group. If the `userType` is `gainAccess`, they gain access to the resources in the source group. Users who have inherited access to a group lose access to that group if it moves out of its hierarchy that gives them those access rights. If the group moves to another parent group to which they have access, they still have access to the group you move. Likewise, when you move a group to a new location, users who inherit their access rights from the new parent group gain access to the resources in the group you move.\n",
        "parameters": [
          {
            "name": "userType",
            "in": "query",
            "description": "Filters the list by users who have `lostAccess` or the reverse `gainAccess`.\n",
            "schema": {
              "type": "string",
              "example": "lostAccess",
              "enum": [
                "lostAccess",
                "gainAccess"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The response provides the affected users.\n",
            "content": {
              "application/json": {
                "example": [
                  {
                    "uiIdentityId": "A-B-12345",
                    "firstName": "John",
                    "lastName": "Smith",
                    "accountId": "A-CCT7890",
                    "email": "jsmith@example.com",
                    "uiUserName": "jsmith@example.com",
                    "lastLoginDate": "2017-08-03T21:15:27.000Z"
                  },
                  {
                    "uiIdentityId": "1-2A3BCD",
                    "firstName": "Jan",
                    "lastName": "Kowalski",
                    "accountId": "A-CCT1234",
                    "email": "jkowalski@example.com",
                    "uiUserName": "jkowalski@example.com",
                    "lastLoginDate": "2016-09-07T00:00:00.000Z"
                  }
                ],
                "schema": {
                  "type": "array",
                  "minItems": 0,
                  "items": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": [
                      "accountId",
                      "email",
                      "firstName",
                      "lastName",
                      "uiIdentityId",
                      "uiUserName"
                    ],
                    "properties": {
                      "accountId": {
                        "type": "string",
                        "description": "Unique identifier for each account.\n",
                        "minLength": 1
                      },
                      "email": {
                        "type": "string",
                        "description": "The user's email address.\n",
                        "format": "email"
                      },
                      "firstName": {
                        "type": "string",
                        "description": "The user's first name.\n",
                        "minLength": 1
                      },
                      "lastLoginDate": {
                        "type": "string",
                        "description": "ISO 8601 timestamp indicating when the user last logged in.\n",
                        "format": "date-time"
                      },
                      "lastName": {
                        "type": "string",
                        "description": "The user's surname.\n",
                        "minLength": 1
                      },
                      "uiIdentityId": {
                        "type": "string",
                        "description": "Unique identifier for each user, which corresponds to their Control Center profile or client ID. Also known as a `contactId` in other APIs.\n",
                        "minLength": 1
                      },
                      "uiUserName": {
                        "type": "string",
                        "description": "The user's username in Control Center.\n",
                        "minLength": 1
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
